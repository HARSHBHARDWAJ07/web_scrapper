{
  "name": "project_scrapper_js",
  "version": "2.1.0",
  "description": "High-performance Instagram scraper using Apify API with Express, caching, and rate limiting",
  "main": "server.mjs",
  "type": "module",
  "scripts": {
    "start": "node server.mjs",
    "dev": "nodemon --watch server.mjs --ext mjs,js,json",
    "prod": "NODE_ENV=production node server.mjs",
    "test": "echo \"No tests specified\" && exit 0",
    "benchmark": "autocannon -c 50 -d 15 http://localhost:3000/scrape",
    "lint": "eslint . --ext .js,.mjs"
  },
  "keywords": [
    "scraper",
    "apify",
    "instagram",
    "express",
    "nodejs",
    "social-media",
    "performance",
    "caching",
    "rate-limiting"
  ],
  "author": "Harsh Bhardwaj",
  "license": "ISC",
  "dependencies": {
    "apify-client": "^2.9.3",
    "express": "^4.21.1",
    "pino": "^9.4.0",
    "node-cache": "^5.1.2",
    "limiter": "^2.1.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.0",
    "eslint": "^8.57.0",
    "autocannon": "^7.14.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/project_scrapper_js.git"
  },
  "bugs": {
